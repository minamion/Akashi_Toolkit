<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/pink_primary</item>
        <item name="colorPrimaryDark">@color/pink_primary_dark</item>
        <item name="colorAccent">@color/pink_accent</item>
        <item name="icon">@color/pink_icons</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <!-- 明石红-->
    <style name="AppTheme.NoActionBar.pink" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/pink_primary</item>
        <item name="colorPrimaryDark">@color/pink_primary_dark</item>
        <item name="colorAccent">@color/pink_accent</item>
        <item name="icon">@color/pink_icons</item>
    </style>
    <!-- 夕张青-->
    <style name="AppTheme.NoActionBar.teal">
        <item name="colorPrimary">@color/teal_primary</item>
        <item name="colorPrimaryDark">@color/teal_primary_dark</item>
        <item name="colorAccent">@color/teal_accent</item>
        <item name="icon">@color/teal_icons</item>
    </style>
    <!-- 大淀蓝-->
    <style name="AppTheme.NoActionBar.blue">
        <item name="colorPrimary">@color/blue_primary</item>
        <item name="colorPrimaryDark">@color/blue_primary_dark</item>
        <item name="colorAccent">@color/blue_accent</item>
        <item name="icon">@color/blue_icons</item>
    </style>
    <!-- 哈酱黄-->
    <style name="AppTheme.NoActionBar.yellow">
        <item name="colorPrimary">@color/yellow_primary</item>
        <item name="colorPrimaryDark">@color/yellow_primary_dark</item>
        <item name="colorAccent">@color/yellow_accent</item>
        <item name="icon">@color/yellow_icons</item>
    </style>
    <!-- 瑞鹤绿-->
    <style name="AppTheme.NoActionBar.green">
        <item name="colorPrimary">@color/green_primary</item>
        <item name="colorPrimaryDark">@color/green_primary_dark</item>
        <item name="colorAccent">@color/green_accent</item>
        <item name="icon">@color/green_icons</item>
    </style>
    <!--秋津灰-->
    <style name="AppTheme.NoActionBar.grey">
        <item name="colorPrimary">@color/grey1_primary</item>
        <item name="colorPrimaryDark">@color/grey1_primary_dark</item>
        <item name="colorAccent">@color/grey1_accent</item>
        <item name="icon">@color/grey1_icons</item>
    </style>
    <!--欧根黄-->
    <style name="AppTheme.NoActionBar.orange">
        <item name="colorPrimary">@color/orange_primary</item>
        <item name="colorPrimaryDark">@color/orange_primary_dark</item>
        <item name="colorAccent">@color/orange_accent</item>
        <item name="icon">@color/orange_icons</item>
    </style>
</resources>
